<form  action="/topics/3" method="post">
          
    <input type="text" placeholder="Source Title" name="sourceTitle" />
    <input type="text" placeholder="Link" name="link" />
    <textarea rows="4" cols="10" placeholder="description" name="description"></textarea>
    <input value="<%=category%>" type="text" placeholder="referenceID" name="referenceID"/>
      <button type="submit"   data-dismiss="modal" class="btn btn-primary">Create</button>
</form>
<table class="topics-container">
<tr>
  <th>sourceTitle</th>
  <th>decription</th>
  <th>link</th>
  <th>delete</th>
  <th>Edit</th>
</tr>
<% topics.forEach(topic=>{%>



<tr>
<td><%=topic.sourceTitle%></td>
<td><p><%=topic.description%></p></td>
<td><a href="<%=topic.link%>" target="_blank">link</a></td>
<td><a style="color:red;" href="/topics/del/3/<%=topic.id%>">Delete</a></td>
<td>
<a href="/topics/update/3/<%=topic.id%>">
  Edit Topic </a></td>

</tr>

<%})%>
